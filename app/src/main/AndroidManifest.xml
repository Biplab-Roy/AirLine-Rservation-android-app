<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.biplabroy.airreservation">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Air Line Reservation"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".RegistrationActivity1" />
        <activity android:name=".StatusActivity" />
        <activity android:name=".RegistrationActivity2" />
        <activity android:name=".AdminActivity" />
        <activity android:name=".ListActivity" />
        <activity android:name=".StatusLoginActivity" />
        <activity android:name=".EditRegistrationActivity1" />
        <activity android:name=".EditRegistrationActivity2" />
        <activity android:name=".SpalashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GenerateQRActivity" />
        <activity android:name=".FlightAdapter" />
        <activity android:name=".FlightDetails" />
        <activity android:name=".FlightActivity" />
        <activity android:name=".AddFlightActivity"></activity>
    </application>

</manifest>